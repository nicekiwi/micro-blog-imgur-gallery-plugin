{
    "version": "1.0.0",
    "title": "Imgur Gallery",
    "description": "Inject imgur albums as galleries.",
    "includes": [
        "/app.js",
        "/app.css",
        "imagur.html"
    ],
    "fields": [
        {
            "field": "params.imgur_client_id",
            "label": "Imgur Client ID",
            "placeholder": ""
        },
        {
            "field": "params.custom_stylesheet",
            "label": "Use your own CSS?",
            "placeholder": "false"
        },
        {
            "field": "params.custom_lightbox",
            "label": "Use a custom Lightbox?",
            "placeholder": "false"
        }
    ]
}